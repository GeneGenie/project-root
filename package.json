{
  "name": "learnbuild",
  "version": "1.0.0",
  "description": "template project for a lib",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "build": "NODE_ENV=prod rollup -c build/build.js",
    "dev:be": "node server/index.js",
    "dev:fe": "NODE_ENV=dev rollup -c build/serve.dev.js -w",
    "prod:fe": "NODE_ENV=prod rollup -c build/serve.prod.js -w",
    "dev": "run-p dev:be dev:fe",
    "start": "run-p dev:be prod:fe",
    "report": "rollup -c build/report.js",
    "dev:lint": "eslint src/** build/** --rulesdir eslintLocal"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "cors": "^2.8.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-local": "file:eslintLocal",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-rulesdir": "^0.2.2",
    "globals": "^15.0.0",
    "gzip-size": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rollup": "^3.0.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-visualizer": "^5.12.0"
  },
  "dependencies": {
    "express": "^4.19.2",
    "rollup-plugin-dotenv": "^0.5.0",
    "web-vitals": "^3.5.2"
  }
}
